{
  "_new_lines": "\n",
  "_selenide_pom_version": "0.26.0",
  "_selenide_version": "7.0.4",
  "name": "my_selenide_pom_project",
  "repo_name": "{{ cookiecutter.name | lower | replace(' ', '_') }}",
  "group": "my.group.{{ cookiecutter.repo_name | replace('-', '') | replace('_', '') }}",
  "__group_folder": "{{ cookiecutter.group | replace('.', '/') }}",
  "use_appium": [
    "no",
    "yes"
  ],
  "use_cucumber": [
    "no",
    "yes"
  ],
  "_copy_without_render": [
    ".idea/*",
    "gradle/*",
    "gradlew",
    "gradlew.bat",
    "gradle.properties",
    ".gitignore"
  ]
}
